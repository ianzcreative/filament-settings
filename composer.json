{
    "name": "ianstudios/filament-settings",
    "description": "This package adds a way to interact with key:value settings in Filament.",
    "homepage": "https://github.com/ianzcreative/filament-settings",
    "license": "MIT",
    "authors": [
        {
            "name": "Ianstudios",
            "email": "info@ianstudios.id"
        }
    ],
    "require": {
        "php": "^8.0",
        "filament/filament": "^3.2",
        "laravel/framework": "^10.0|^11.0|^12.0",
        "Ianstudios/settings": "^1.0",
        "spatie/laravel-package-tools": "^1.16"
    },
    "autoload": {
        "psr-4": {
            "Ianstudios\\FilamentSettings\\": "src/"
        },
        "files": [
            "src\\Helpers\\helpers.php"
        ]
    },
    "extra": {
        "laravel": {
            "providers": [
                "Ianstudios\\FilamentSettings\\FilamentSettingsServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
